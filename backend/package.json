{
    "name": "task-manager-backend",
    "version": "1.0.1",
    "description": "Backend API for Task Manager",
    "author": "Your Name <abid.syeed@napses.com>",
    "private": true,
    "license": "UNLICENSED",
    "tag": "task-manager-backend tag",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "load-secrets": "ts-node scripts/load-secrets.ts",
        "start": "nest start --watch",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "gts lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "coverage:bitbucket": "NODE_ENV=ci nyc --reporter=lcov ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "npm run compile",
        "clean": "gts clean",
        "compile": "tsc",
        "fix": "gts fix",
        "pretest": "npm run compile",
        "posttest": "npm run lint",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source.ts",
        "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/typeorm.config.ts migration:generate ./src/migrations/$npm_config_name",
        "migration:create": "npm run typeorm -- migration:generate src/migration/migration_name -d src/database/data-source.ts",
        "migration:revert": "NODE_ENV=${npm_config_env} npm run typeorm -- -d ./src/config/typeorm.config.ts migration:revert",
        "migration:show": "NODE_ENV=${npm_config_env} npm run typeorm -- -d ./src/config/typeorm.config.ts migration:show",
        "migration:up": "NODE_ENV=${npm_config_env} npm run typeorm -- -d ./src/config/typeorm.config.ts migration:up",
        "seed": "ts-node src/seeders/user.seeder.ts",
        "coverage": "nyc npm run test:cov",
        "release": "standard-version",
        "sqs:create:dlq": "NODE_ENV=${npm_config_env} ts-node -r tsconfig-paths/register ./src/utils/aws/sqs/create-dead-letter-queue.ts",
        "sqs:create": "NODE_ENV=${npm_config_env} ts-node -r tsconfig-paths/register ./src/utils/aws/sqs/create-queues.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.600.0",
        "@aws-sdk/client-secrets-manager": "^3.600.0",
        "@aws-sdk/client-sqs": "^3.600.0",
        "@aws-sdk/credential-providers": "^3.600.0",
        "@aws-sdk/s3-request-presigner": "^3.729.0",
        "@faker-js/faker": "^8.4.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/serve-static": "^4.0.2",
        "@nestjs/swagger": "^7.3.1",
        "@nestjs/terminus": "^10.2.3",
        "@nestjs/typeorm": "^10.0.2",
        "@types/geoip-lite": "^1.4.4",
        "@types/nodemailer": "^6.4.15",
        "aws-sdk": "^2.1640.0",
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "chai": "^4.4.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cls-hooked": "^4.2.2",
        "config": "^3.3.11",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "folktale": "^2.3.2",
        "form-data": "^4.0.1",
        "geoip-lite": "^1.4.10",
        "helmet": "^7.1.0",
        "ioredis": "^5.4.1",
        "mailgun.js": "^10.2.4",
        "nestjs-i18n": "^10.4.8",
        "nestjs-pino": "^4.1.0",
        "newrelic": "^11.22.0",
        "node-mocks-http": "^1.15.0",
        "nodemailer": "^6.9.14",
        "nodemailer-mock": "^2.0.6",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.13.1",
        "pino": "^9.1.0",
        "pino-multi-stream": "^6.0.0",
        "rate-limiter-flexible": "^5.0.3",
        "rxjs": "^7.8.1",
        "sanitize-html": "^2.13.1",
        "shortid": "^2.2.16",
        "sinon": "^18.0.0",
        "sinon-chai": "^3.7.0",
        "sqlite3": "^5.1.7",
        "sqs-consumer": "^11.4.0",
        "typeorm": "^0.3.20",
        "uuid": "^10.0.0",
        "uuidv7": "^1.0.1",
        "winston": "^3.13.0",
        "winston-cloudwatch": "^6.3.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cls-hooked": "^4.3.8",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.12",
        "@types/multer": "^1.4.11",
        "@types/newrelic": "^9.14.4",
        "@types/node": "20.12.7",
        "@types/passport": "^1.0.16",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/sanitize-html": "^2.13.0",
        "@types/shortid": "^0.0.32",
        "@types/supertest": "^6.0.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "gts": "^5.3.1",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "pino-pretty": "^11.2.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "standard-version": "^9.5.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.2.2",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.3"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "test",
            "**/*.spec.ts",
            "src/main.ts"
        ],
        "reporter": [
            "html",
            "text",
            "text-summary"
        ],
        "extension": [
            ".ts"
        ]
    }
}